<%- include ("partials/header.ejs"); %>

    <!-- display all rooms, reditect to /room_name on join-->
    <div id="room-container">
        <% Object.keys(rooms).forEach(room => { %>
        <div><%= room %></div>
        <a href = "/<%= room %>">Join</a>
        <% }) %>
    </div>


    <form action="/club" method="POST">
        <input name="user_you" type="text" required>
        <input name="user_friend" type="text" required>
        <button type="submit">Create New Room</button>
    </form>

    <script defer type="text/javascript" src="http://localhost:3000/socket.io/socket.io.js"></script>
    <script defer type="text/javascript" src="script.js"></script>

    <%- include ("partials/footer.ejs"); %>
